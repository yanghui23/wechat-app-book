<!--components/navi/index.wxml-->

<!-- 移动端一定要考虑用户可用手指点击的范围 -->

<view class="container">
  <image class="icon" src="{{ latest ? disLeftSrc : leftSrc }}" bind:tap="onLeft" />
  <text class="title">{{ title }}</text>
  <image class="icon" src="{{ first ? disRightSrc : rightSrc }}" bind:tap="onRight" />
</view>
